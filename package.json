{
    "name": "phaser-project-template",
    "version": "3.55.2",
    "description": "Phaser 3 starter template with TypeScript and webpack.",
    "homepage": "https://github.com/yandeu/phaser-project-template#readme",
    "main": "index.js",
    "scripts": {
        "start": "webpack serve --config webpack/webpack.dev.js",
        "build": "rimraf dist && webpack --config webpack/webpack.prod.js",
        "bundle": "npm run build",
        "serve": "serve dist",
        "format": "prettier --check src/scripts/**",
        "format:write": "prettier --write src/scripts/**",
        "atlasGen": "node src/scripts/utils/atlasGen.js",
        "deploy": "gh-pages -d dist"
    },
    "keywords": [
        "Phaser",
        "Phaser 3",
        "Phaser3",
        "html5 game",
        "TypeScript",
        "webpack",
        "starter"
    ],
    "author": {
        "name": "Yannick Deubel",
        "url": "https://github.com/yandeu"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ntntn/homm_guys.git"
    },
    "template": {
        "name": "phaser-project-template",
        "url": "https://github.com/yandeu/phaser-project-template",
        "author": "Yannick Deubel (https://github.com/yandeu)"
    },
    "engines": {
        "node": ">=12"
    },
    "license": "MIT",
    "dependencies": {
        "@trixt0r/ecs": "^0.4.6",
        "@types/dat.gui": "^0.7.7",
        "dat.gui": "^0.7.9",
        "folder-hash": "^4.0.2",
        "free-tex-packer-core": "^0.3.4",
        "json-loader": "^0.5.7",
        "node-json-db": "^1.5.0",
        "phaser": "^3.55.2",
        "phaser3-nineslice": "^0.5.0",
        "simplex-noise": "^3.0.1",
        "tweakpane": "^3.0.8"
    },
    "devDependencies": {
        "@tweakpane/core": "^1.0.9",
        "@yandeu/prettier-config": "^0.0.3",
        "copy-webpack-plugin": "^10.1.0",
        "css-loader": "^6.6.0",
        "gh-pages": "^5.0.0",
        "html-webpack-plugin": "^5.5.0",
        "javascript-obfuscator": "^3.0.0",
        "mini-css-extract-plugin": "^2.5.3",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "sass": "^1.49.7",
        "sass-loader": "^12.4.0",
        "serve": "^13.0.2",
        "ts-loader": "^9.2.6",
